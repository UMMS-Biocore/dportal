{
  "name": "dportal",
  "version": "1.0.0",
  "description": "dportal server",
  "main": "app.js",
  "scripts": {
    "start:dev": "nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "watch:js2": "parcel watch ./public/js/index.js --out-dir ./public/dist",
    "watch:js": "parcel watch ./public/js/index.js --public-url /js --out-dir ./public/js --out-file bundle.js",
    "build:js": "parcel build ./public/js/index.js --public-url /js --out-dir ./public/js --out-file bundle.js",
    "dev": "concurrently \"npm run start:dev\" \"npm run watch:js2\"",
    "debug": "ndb nodemon --exec \"node server.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Alper Kucukural, Onur Yukselen",
  "license": "ISC",
